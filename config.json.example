{
  "maxDownloads": 100,
  "minWidth": 800,
  "minHeight": 600,
  "minFileSize": "100KB",
  "fileTypes": [
    "jpg",
    "jpeg",
    "png",
    "gif",
    "webp"
  ],
  "platformSpecific": {
    "windows": {
      "selectedDrives": [
        "C:\\"
      ],
      "scanNetworkDrives": false
    },
    "darwin": {
      "defaultScanPath": "/Users/your_username/Pictures"
    },
    "linux": {
      "defaultScanPath": "/home/your_username/Pictures"
    }
  },
  "providers": {
    "google": { "enabled": false, "maxResults": 100 },
    "pexels": { "enabled": true, "maxResults": 30 },
    "bing": {
      "enabled": true,
      "maxResults": 30,
      "maxScrollsBing": 15,
      "scrollDelayBing": 2000,
      "lightboxDelayBing": 1500
    },
    "flickr": {
      "enabled": false,
      "maxResults": 30,
      "apiKey": "YOUR_FLICKR_API_KEY_HERE"
    },
    "duckduckgo": {
      "enabled": true,
      "maxResults": 30,
      "maxScrollsDDG": 15,
      "loadMoreTimeoutDDG": 10000,
      "scrollDelayDDG": 2000
    },
    "freeimages": {
      "enabled": true,
      "displayName": "FreeImages.com",
      "maxResults": 30,
      "scrolling": {
        "maxScrolls": 10,
        "scrollDelay": 2500,
        "noNewImagesRetries": 3,
        "useAutoScroll": true
      },
      "notes": "Scraping-based provider using GenericPlaywrightProvider."
    },
    "wikimedia": { "enabled": true, "maxResults": 30 },
    "pixabay": { "enabled": true, "maxResults": 30 },
    "unsplash": { "enabled": true, "maxResults": 30 },
    "stocksnap": {
      "enabled": true,
      "maxResults": 25,
      "comment": "Free CC0 images from StockSnap.io"
    },
    "shutterstock": {
      "enabled": true,
      "maxResults": 20,
      "apiKey": "YOUR_SHUTTERSTOCK_API_KEY_OR_LEAVE_EMPTY_FOR_PREVIEWS",
      "comment": "Fetches watermarked previews if no API key. Full API download not yet implemented."
    },
    "freerangestock": {
      "enabled": true,
      "maxResults": 30,
      "comment": "Free stock photos from FreeRangeStock.com"
    },
    "publicdomainpictures": {
      "enabled": true,
      "maxResults": 30,
      "comment": "Public domain and free stock photos from PublicDomainPictures.net"
    },
    "reshot": {
      "enabled": true,
      "maxResults": 30,
      "comment": "Free stock photos from Reshot.com"
    },
    "adobestock": {
      "enabled": true,
      "displayName": "Adobe Stock",
      "apiKey": "",
      "maxResults": 30,
      "comment": "Adobe Stock images (watermarked previews without API key). Add your API key for better results.",
      "notes": "Works with or without an API key. Without an API key, it uses web scraping to download preview images."
    },
    "gettyimages": {
      "enabled": false,
      "displayName": "Getty Images",
      "apiKey": "YOUR_GETTY_IMAGES_API_KEY",
      "maxResults": 30,
      "notes": "Requires an API key from Getty Images developer portal. Provides watermarked previews via API."
    },
    "dreamstime": {
      "enabled": false,
      "displayName": "Dreamstime.com",
      "apiKey": "YOUR_DREAMSTIME_API_KEY",
      "maxResults": 30,
      "notes": "Requires an API key. API documentation access must be requested from Dreamstime."
    },
    "istock": {
      "enabled": false,
      "displayName": "iStock (via Getty API)",
      "apiKey": "YOUR_GETTY_IMAGES_API_KEY",
      "maxResults": 30,
      "notes": "Uses the Getty Images API. Requires the same API key as Getty Images."
    },
    "500px": {
      "enabled": false,
      "displayName": "500px",
      "apiKey": "YOUR_500PX_CONSUMER_KEY",
      "maxResults": 20,
      "notes": "Requires a Consumer Key for the (legacy) 500px API."
    },
    "stocksy": {
      "enabled": false,
      "displayName": "Stocksy",
      "apiKey": "YOUR_STOCKSY_API_KEY",
      "maxResults": 30,
      "notes": "Requires an API key. API documentation may need to be requested from Stocksy."
    },
    "alamy": {
      "enabled": false,
      "displayName": "Alamy",
      "apiKey": "YOUR_ALAMY_API_KEY",
      "maxResults": 30,
      "notes": "Requires an API key. API documentation at https://api-reference.alamy.com/ (requires JavaScript)."
    },
    "bigstock": {
      "enabled": false,
      "displayName": "Bigstock",
      "apiKey": "YOUR_BIGSTOCK_API_KEY_IF_AVAILABLE",
      "maxResults": 30,
      "notes": "API documentation not readily found. May require direct contact with Bigstock or be scraping-only."
    },
    "pond5": {
      "enabled": false,
      "displayName": "Pond5",
      "apiKey": "YOUR_POND5_API_KEY",
      "maxResults": 30,
      "notes": "Requires an API key. API documentation needs to be requested from Pond5 (https://www.pond5.com/api). Check if API is photo-specific or general media."
    },
    "depositphotos": {
      "enabled": false,
      "displayName": "Depositphotos",
      "apiKey": "YOUR_DEPOSITPHOTOS_API_KEY",
      "maxResults": 30,
      "notes": "Requires an API key. API documentation needs to be requested from Depositphotos (https://depositphotos.com/api-suite.html)."
    },
    "littlevisuals": {
      "enabled": false,
      "displayName": "Little Visuals (Archive)",
      "maxResults": 100, 
      "notes": "Archive site, provides a fixed set of images. Search query is ignored."
    },
    "newoldstock": {
      "enabled": false,
      "displayName": "New Old Stock",
      "maxResults": 50,
      "notes": "Vintage photos from public archives. Configuration is a placeholder due to robots.txt blocking inspection and needs manual verification by the user."
    }
  },
  "logLevel": "INFO"
}
